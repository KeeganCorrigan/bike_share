<%= form_for @trip, url: admin_trip_path(@trip) do |f| %>

  <%= f.label :duration %>
  <%= f.number_field :duration %>

  <%= f.label :start_date %>
  <%= f.text_field :start_date %>

  <%= f.label :start_station %>
  <%= f.collection_select :start_station%>

  <%= f.label :end_date %>
  <%= f.text_field :end_date %>

  <%= f.label :end_station %>
  <%= f.collection_select :end_trips, Station.all  %>

  <%= f.label :subscription_type %>
  <%= f.text_field :subscription_type %>

  <%= f.label :zip_code %>
  <%= f.text_field :zip_code %>

  <%= f.label :bike_id %>
  <%= f.number_field :bike_id %>

  <%= f.submit "Update Trip" %>

<% end %>
