<h1> Trips</h1>

<% @trips.each do |trip| %>
  <div class="trip-row row">
    <p><%= trip.duration %></p>
    <p><%= trip.start_station.name %></p>
    <p><%= trip.start_date %></p>
    <p><%= trip.end_station.name %></p>
    <p><%= trip.end_date %></p>
    <p><%= trip.subscription_type %></p>
    <p><%= trip.zip_code %></p>
  </div>
<% end %>
